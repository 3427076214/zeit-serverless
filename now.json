{
  "version": 2,
  "functions": {
    "api/version/index.go": {
      "maxDuration": 5,
      "includeFiles": "data.db"
    },
    "api/upsgo/main.go": {
      "maxDuration": 5
    }
  },
  "routes": [
    { "src": "/api/go/version/get", "dest": "api/version/index.go" },
    { "src": "/api/go/upsgo", "dest": "api/upsgo/main.go" },
    { "src": "/", "dest": "index.html" },
    { "src": "/api", "dest": "index.html" },
    { "handle": "filesystem" },
    { "src": "/upsgo/css/(.*)", "dest": "dist/upsgo/css/$1" },
    { "src": "/upsgo/js/(.*)", "dest": "dist/upsgo/js/$1" },
    { "src": "/upsgo", "dest": "dist/upsgo/index.html" },
    { "src": "/upsgo/(.*)", "dest": "dist/upsgo/index.html" }
  ]
}